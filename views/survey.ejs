  <script>
    // This script will be executed in the browser
    // It shows an alert and resets the form on successful submission
    const form = document.getElementById("surveyForm");

    form.addEventListener("submit", async (event) => {
        event.preventDefault(); // Prevent the form from submitting normally

        // Perform the form submission using Fetch API or other methods if needed
        const response = await fetch("/addRecord", {
            method: "post",
            body: new FormData(form),
        });

        if (response.ok) {
            alert("Survey submitted successfully!");
            form.reset(); // Reset the form
        } else {
            alert("Survey submission failed!");
        }
    });
